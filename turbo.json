{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["lint", "^build"],
            "outputs": [".next/**"]
        },
        "dev": {
            "cache": false,
            "outputMode": "none"
        },
        "lint": {
            "cache": false
        },
        "deploy": {
            "dependsOn": ["build"]
        },
        "production": {
            "dependsOn": ["build"]
        },
        "start": {
            "dependsOn": ["build"]
        }
    }
    
}